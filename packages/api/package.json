{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "albumIds": "bun run ./src/modules/google-photos/fetch-album-ids.ts",
    "authorize": "bun run ./src/modules/google-calendar/authorize.ts",
    "build": "tsc",
    "calendar": "bun run ./src/modules/google-calendar/test-google-calendar.ts",
    "clean": "rm -rf dist",
    "dev": "dotenv -- nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "generate-message": "bun run ./src/modules/message/generate-message.ts",
    "generate-spotify-types": "./node_modules/.bin/oazapfts --optimistic ./fixed-spotify-open-api.yml ./src/modules/spotify/spotify.api.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "photos": "bun run ./src/modules/google-photos/test-google-photos.ts",
    "spotify": "bun run ./src/modules/spotify/test-spotify.ts",
    "start": "dotenv -- node -r esbuild-register ./src/index.ts",
    "weather": "bun run ./src/modules/weather/test-weather.ts"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron": "^2.4.4",
    "dotenv-cli": "^7.3.0",
    "googleapis": "^129.0.0",
    "openai": "^3.3.0",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0",
    "ws": "^8.15.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/node": "^18.19.3",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.18.20",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-config-mirror": "workspace:*",
    "nodemon": "^3.0.2",
    "oazapfts": "^4.12.0",
    "tsconfig": "workspace:*",
    "typescript": "^5.3.3"
  }
}
