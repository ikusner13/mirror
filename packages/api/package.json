{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "albumIds": "ts-node ./src/modules/google-calendar/fetch-album-ids.ts",
    "authorize": "ts-node ./src/modules/google-calendar/authorize.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "getPhotos": "ts-node ./src/modules/google-calendar/fetch-photos.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node -r esbuild-register ./src/index.ts"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron": "^2.4.0",
    "googleapis": "^123.0.0",
    "morgan": "^1.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.18",
    "@types/ws": "^8.5.5",
    "esbuild": "^0.18.17",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.46.0",
    "eslint-config-mirror": "workspace:*",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  }
}
